{
  "manifest_version": 3,
  "name": "Translator Extension",
  "version": "1.0",
  "description": "A simple translator extension",
  "permissions": ["activeTab", "scripting"],
  "action":{
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}